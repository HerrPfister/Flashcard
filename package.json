{
  "name": "flashcard",
  "version": "0.0.1",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "UNLICENSED",
  "private": false,
  "repository": "git@github.com:HerrPfister/Flashcard.git",
  "author": "Matthew_Pfister <herrpfister@gmail.com>",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.17",
    "@mdx-js/react": "^1.6.18",
    "@storybook/addon-essentials": "^6.1.1",
    "@storybook/addons": "^6.1.1",
    "@storybook/components": "^6.1.1",
    "@storybook/react": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@types/babel__core": "^7",
    "@types/chance": "^1.1.1",
    "@types/css-mediaquery": "^0.1.0",
    "@types/jest": "^26.0.20",
    "@types/jss": "^10.0.0",
    "@types/mdx-js__react": "^1",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "chance": "^1.1.7",
    "core-js": "^3.6.5",
    "eslint": "^7.9.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.0.3",
    "webpack": "4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "clsx": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint ./src",
    "test": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/build/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "resetMocks": true,
    "verbose": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coverageReporters": [
      "lcov",
      "cobertura",
      "clover",
      "json"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/coverage/"
    ]
  }
}
